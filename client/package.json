    "file-loader": "^6.0.0",
    "html-loader": "^1.1.0",
    "html-webpack-plugin": "^4.2.0",
    "jest": "^26.2.2",
    "react-hot-loader": "^4.12.21",
    "redux-mock-store": "^1.5.4",
    "redux-promise-middleware": "^6.1.2",
